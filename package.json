{
  "name": "@whazzark/cyclone-template-back",
  "version": "0.1.2",
  "author": "Romain Petit",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/whazzark/cyclone.git",
    "directory": "packages/cyclone-template-back"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "start": "env-cmd -f .env next dev",
    "premigration:dev": "env-cmd -f .env.dev prisma migrate dev --preview-feature",
    "migration:dev": "env-cmd -f .env.dev prisma migrate deploy --preview-feature",
    "build:dev": "env-cmd -f .env.dev next build",
    "build:staging": "env-cmd -f .env.staging next build",
    "premigration:staging": "env-cmd -f .env.staging prisma migrate dev --preview-feature",
    "migration:staging": "env-cmd -f .env.staging prisma migrate deploy --preview-feature",
    "build:production": "env-cmd -f .env.production next build",
    "premigration:production": "env-cmd -f .env.production prisma migrate dev --preview-feature",
    "migration:production": "env-cmd -f .env.production prisma migrate deploy --preview-feature"
  },
  "dependencies": {
    "@ant-design/icons": "4.5.0",
    "@premieroctet/next-crud": "1.1.0",
    "@prisma/client": "2.19.0",
    "@zeit/next-less": "1.0.1",
    "antd": "4.13.0",
    "axios": "0.21.1",
    "babel-plugin-import": "1.7.0",
    "env-cmd": "10.1.0",
    "less": "4.1.1",
    "less-vars-to-js": "1.3.0",
    "lodash": "4.17.21",
    "next": "10.0.7",
    "null-loader": "2.0.0",
    "prisma": "2.19.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "swr": "0.5.3",
    "webpack": "4.46.0"
  }
}
